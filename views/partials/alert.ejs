<% if (locals.messages && messages.message) {
	message = messages.message[0];%>
	<div id="messageAlert" class="alert alert-<%=message.type%> alert-dismissible fade show" role="alert">
		<strong><%= message.title %>:</strong> <%=message.text%>
		<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
	</div>
<% } %>